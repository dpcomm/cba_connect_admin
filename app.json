{
  "expo": {
    "name": "CBA Connect 관리자",
    "slug": "cba_connect_admin",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon_admin_app.png",
    "scheme": "cbaconnectadmin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "3"
    },
    "android": {
      "icon": "./assets/images/icon_admin_app.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cba.cba_connect_application_admin",
      "versionCode": 3
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "updates": {
      "url": "https://u.expo.dev/c1133bdb-8c42-46e6-ba94-82b80ea38b84"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c1133bdb-8c42-46e6-ba94-82b80ea38b84"
      }
    }
  }
}
